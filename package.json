{
  "name": "paw-health",
  "private": true,
  "scripts": {
    "dev:all": "turbo dev",
    "dev:api": "turbo dev --filter=@paw-health/api",
    "dev:web": "turbo dev --filter=@paw-health/web",
    "dev:landing": "turbo dev --filter=@paw-health/landing",
    "dev:mobile": "pnpm --filter @paw-health/mobile start",
    "mobile:android": "pnpm --filter @paw-health/mobile android",
    "mobile:ios": "pnpm --filter @paw-health/mobile ios",
    "mobile:web": "pnpm --filter @paw-health/mobile web",
    "build": "turbo build",
    "build:api": "turbo build --filter=@paw-health/api",
    "build:web": "turbo build --filter=@paw-health/web",
    "build:landing": "turbo build --filter=@paw-health/landing",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:api": "turbo lint --filter=@paw-health/api",
    "lint:web": "turbo lint --filter=@paw-health/web",
    "lint:landing": "turbo lint --filter=@paw-health/landing",
    "lint:mobile": "pnpm --filter @paw-health/mobile lint",
    "add:api": "pnpm --filter @paw-health/api add",
    "add:web": "pnpm --filter @paw-health/web add",
    "add:landing": "pnpm --filter @paw-health/landing add",
    "add:mobile": "pnpm --filter @paw-health/mobile add",
    "add:api:dev": "pnpm --filter @paw-health/api add -D",
    "add:web:dev": "pnpm --filter @paw-health/web add -D",
    "add:landing:dev": "pnpm --filter @paw-health/landing add -D",
    "add:mobile:dev": "pnpm --filter @paw-health/mobile add -D",
    "add:root": "pnpm add -w",
    "add:root:dev": "pnpm add -Dw",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "prettier": "^3.4.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
